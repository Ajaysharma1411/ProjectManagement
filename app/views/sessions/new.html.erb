
<% provide(:title, "LOG IN") %>
<p>New user? <%= link_to "Sign up now!",  new_user_path %></p>
<h2>LOG IN</h2>


<%= form_for(:session, url: sessions_path) do |f| %>

<%= f.label :email  %> 

<%= f.text_field :email , placeholder: "Email" %><br><br>

<%= f.label :password %>
<%= f.password_field :password , placeholder: "Password" %><br><br>

<%= f.submit "Log in" %>
<% end %>


